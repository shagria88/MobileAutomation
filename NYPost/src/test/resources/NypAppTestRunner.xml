<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Android test suit on Android Real Phone">

    <parameter name="OS" value="Android"/>
    <parameter name="appType" value="Phone"/>
    <parameter name="deviceType" value="RealDevice"/>
    <parameter name="deviceName" value="ONEPLUS A5000"/>
    <parameter name="version" value="8.0.0"/>

    <test name="Go to Home page" preserve-order="true">
        <classes>
            <class name="androidPhone.TestBusiness"></class>
            <class name="androidPhone.TestEntertainment"></class>
            <class name="androidPhone.TestFashion"></class>
            <class name="androidPhone.TestHome"></class>
            <class name="androidPhone.TestLatestStories"></class>
            <class name="androidPhone.TestLiving"></class>
            <class name="androidPhone.TestMedia"></class>
            <class name="androidPhone.TestMetro"></class>
            <class name="androidPhone.TestNews"></class>
            <class name="androidPhone.TestOpinion"></class>
            <class name="androidPhone.TestPageSix"></class>
            <class name="androidPhone.TestRealEstate"></class>
            <class name="androidPhone.TestSports"></class>
            <class name="androidPhone.TestTech"></class>
        </classes>
    </test>
</suite>
