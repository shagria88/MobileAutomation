<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="iOS test suite on iPhone Simulator">

    <parameter name="OS" value="ios"/>
    <parameter name="appType" value="iPhone"/>
    <parameter name="deviceType" value="Simulator"/>
    <parameter name="deviceName" value="iPhone Simulator"/>
    <parameter name="version" value="10.3"/>

    <test name="Go to Home page" preserve-order="true">
        <classes>
            <class name="alertsPage.Alerts"/>
            <class name="buttonsPage.Buttons"/>
            <class name="controlsPage.Controls"/>
            <class name="imgesPage.Images"/>
            <class name="segmentsPage.Segments"/>
            <class name="textFieldsPage.TextFields"/>
            <class name="transitionsPage.Transitions"/>
            <class name="uiCatalogPage.UiCatalog"/>
            <class name="webPage.Web"/>

        </classes>
    </test>
</suite>

